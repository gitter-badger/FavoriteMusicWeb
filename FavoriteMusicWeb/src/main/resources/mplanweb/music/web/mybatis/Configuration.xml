<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>

		<setting name="cacheEnabled" value="true" />
		<setting name="useGeneratedKeys" value="false" />
	</settings>

	<typeAliases>

		<typeAlias alias="Search" type="mplanweb.music.web.search.SearchDTO" />
		<typeAlias alias="Ssearch" type="mplanweb.music.web.source.Ssearch" />
		<typeAlias alias="Ssview" type="mplanweb.music.web.source.Ssview" />
		<typeAlias alias="Ssalbum" type="mplanweb.music.web.source.SsAlbum" />
		<typeAlias alias="Ssartist" type="mplanweb.music.web.source.Ssartist" />
		<typeAlias alias="Sscorp" type="mplanweb.music.web.source.Sscorp" />
		<typeAlias alias="Member" type="mplanweb.music.web.member.Member" />
		<typeAlias alias="MsearchDTO" type="mplanweb.music.web.search.MsearchDTO" />
			<typeAlias alias="BoardDTO" type="mplanweb.music.web.board.BoardDTO" />
			<typeAlias alias="BoardCount" type="mplanweb.music.web.board.BoardCount" />
	</typeAliases>

	<mappers>
		<mapper resource="mplanweb/music/web/mybatis/AdminMemberMapper.xml" />
		<mapper resource="mplanweb/music/web/mybatis/Upload.xml" />
		<mapper resource="mplanweb/music/web/mybatis/Search.xml" />
		<mapper resource="mplanweb/music/web/mybatis/Music.xml" />
		<mapper resource="mplanweb/music/web/mybatis/Member.xml" />
		<mapper resource="mplanweb/music/web/mybatis/Board.xml" />
	</mappers>


	<!-- <mapper resource="mplanweb/music/web/mybatis/excel.xml" /> <mapper 
		resource="mplanweb/music/web/mybatis/Upload.xml" /> -->
</configuration>