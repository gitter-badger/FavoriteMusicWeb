<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mplanweb.music.web.test.MemberMapper">

	<!-- result map -->
	<resultMap type="Member" id="MemberResultMap">
		<result property="name" column="NAME" />
		<result property="email" column="EMAIL" />
		<result property="phone" column="PHONE" />
	</resultMap>

	<!-- id의 경우에는 Mapper.java에 동일하게 사용해야함 -->
	<select id="getMembers" resultMap="MemberResultMap">
		SELECT * FROM mplan_test
	</select>

	<!-- parameterType에는 구문에 전달될 파라미터의 패키지 경로를 포함한 전체 클래스명이나 별칭이 들어간다 -->
	<insert id="insertMember" parameterType="mplanweb.music.web.test.Member">
		INSERT INTO mplan_test VALUES(#{NAME}, #{EMAIL}, #{PHONE})
	</insert>

	<!-- update와 delete는 생량 -->
	<update id=""></update>
	<delete id="deleteMember" parameterType="mplanweb.music.web.test.Member">
		DELETE mplan_test WHERE(name=#{name})
	</delete>
</mapper>